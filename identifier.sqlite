@import "Colors.css";

/* Allgemeiner Stil */
body {
    font-family: Arial, sans-serif;
    background-color: var(--beige);
    margin: 0;
    padding: 0;
}

/* Container-Stil */
.container {
    max-width: 400px;
    margin: 100px auto;
    padding: 20px;
}

/* Formular-Sektion */
.form-section {
    background-color: var(--white);
    padding: 20px;
    border: 1px solid var(--ww-blue-100);
    border-radius: 8px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    text-align: center;
}

.form-section h2 {
    color: var(--ww-darkblue-100);
    margin-bottom: 20px;
    font-size: 24px;
    font-weight: bold;
}

/* Formular-Gruppen */
.form-group {
    margin-bottom: 20px;
    text-align: left;
}

label {
    display: block;
    margin-bottom: 5px;
    font-weight: bold;
    color: var(--ww-darkblue-100);
}

input {
    width: 100%;
    padding: 10px;
    border: 1px solid var(--ww-blue-100);
    border-radius: 4px;
    font-size: 16px;
    box-sizing: border-box;
}

/* Button */
button {
    width: 100%;
    padding: 10px;
    font-size: 18px;
    color: var(--white);
    background-color: var(--button);
    border: none;
    border-radius: 4px;
    cursor: pointer;
    transition: background-color 0.3s;
}

button:hover {
    background-color: var(--button-hover);
}

